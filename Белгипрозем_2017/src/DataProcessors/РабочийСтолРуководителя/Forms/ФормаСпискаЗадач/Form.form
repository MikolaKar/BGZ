    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ЗадачиМне.Исполнитель</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЗадачиПоИсполнителю.Ссылка,
	ЗадачиПоИсполнителю.ПометкаУдаления,
	ЗадачиПоИсполнителю.Дата,
	ЗадачиПоИсполнителю.БизнесПроцесс,
	ЗадачиПоИсполнителю.Наименование,
	ЗадачиПоИсполнителю.Автор,
	ЗадачиПоИсполнителю.Важность,
	ЗадачиПоИсполнителю.СостояниеБизнесПроцесса,
	ЗадачиПоИсполнителю.СрокИсполнения,
	ЗадачиПоИсполнителю.АвторСтрокой,
	ЗадачиПоИсполнителю.Выполнена,
	ЗадачиПоИсполнителю.ПринятаКИсполнению,
	ВЫБОР
		КОГДА ЗадачиПоИсполнителю.СрокИсполнения &lt;> ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЗадачаСоСроком,
	ЗадачиПоИсполнителю.ТочкаМаршрута,
	ЗадачиПоИсполнителю.Исполнитель,
	ЕСТЬNULL(ФлагиОбъектов.Флаг, ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.ПустаяСсылка)) КАК Флаг,
	ВЫБОР
		КОГДА ФлагиОбъектов.Флаг = ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.Красный)
			ТОГДА 1
		КОГДА ФлагиОбъектов.Флаг = ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.Синий)
			ТОГДА 2
		КОГДА ФлагиОбъектов.Флаг = ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.Желтый)
			ТОГДА 3
		КОГДА ФлагиОбъектов.Флаг = ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.Зеленый)
			ТОГДА 4
		КОГДА ФлагиОбъектов.Флаг = ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.Оранжевый)
			ТОГДА 5
		КОГДА ФлагиОбъектов.Флаг = ЗНАЧЕНИЕ(Перечисление.ФлагиОбъектов.Лиловый)
			ТОГДА 6
		ИНАЧЕ 0
	КОНЕЦ КАК НомерФлага
ИЗ
	Задача.ЗадачаИсполнителя.ЗадачиПоИсполнителю КАК ЗадачиПоИсполнителю
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ФлагиОбъектов КАК ФлагиОбъектов
		ПО ЗадачиПоИсполнителю.Ссылка = ФлагиОбъектов.Объект
			И (ФлагиОбъектов.Пользователь = &amp;Пользователь)
ГДЕ
	(ТИПЗНАЧЕНИЯ(ЗадачиПоИсполнителю.БизнесПроцесс) = ТИПЗНАЧЕНИЯ(&amp;ТипБизнесПроцесса)
			ИЛИ &amp;ТипБизнесПроцесса = НЕОПРЕДЕЛЕНО)
	И (ЗадачиПоИсполнителю.ТочкаМаршрута = &amp;ТочкаМаршрута
			ИЛИ &amp;ТочкаМаршрута = НЕОПРЕДЕЛЕНО)</queryText>
      <mainTable>Task.ЗадачаИсполнителя.TasksByExecutive</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
