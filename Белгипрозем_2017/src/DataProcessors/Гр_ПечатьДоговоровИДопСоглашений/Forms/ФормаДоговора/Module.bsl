		селекция.Collapse();             // Вставка, а не замещение
		селекция.Paste();
		
		Для к=2 по СтрЧислоСтрок(МассивЗаказчиков[й]) Цикл
			Реквизиты = Строка(Реквизиты) + СтрПолучитьСтроку(МассивЗаказчиков[й],к);
			Если к <> СтрЧислоСтрок(МассивЗаказчиков[й]) тогда
				Реквизиты = Строка(Реквизиты) + Символы.ВК;
