		Заменить(Док, "%Данныезаказчика2%", Реквизиты2);
		Реквизиты = "";
	КонецЦикла;
#КонецОбласти	

	//Вывод перечень работ в протоколе
	Для каждого Этапа Из ЭтапыДоговора Цикл
		ВидРабот = Строка(ВидРабот) + "«" + Строка(Этапа.НаименованиеРаботы) + "», ";
