	СтрокаЗаказчикиФЛ = "";
	СтрокаИнвесторыФЛ = "";
	СтрокаИнвЗакФЛ    = "";
	
	ЗаказчикИнвестор  = Ложь;
	ИндексИнвЗакЮЛ    = Новый Массив;
	ИндексИнвЗакФЛ    = Новый Массив;
