// Печать конвертов из Исходящих документов
Функция ПечатьКонвертов(ОписаниеКоманды) Экспорт
    #Если НЕ ВебКлиент Тогда
        
        ПараметрыФормы = Новый Структура;
        ПараметрыФормы.Вставить("МассивПисем", ОписаниеКоманды.ОбъектыПечати);

	    ОткрытьФорму("Обработка.ПечатьКонвертов.Форма", ПараметрыФормы,
    		ОписаниеКоманды.ОбъектыПечати);
                
    #Иначе
        Предупреждение(НСтр("ru = 'В веб-клиенте выполнение данной операции невозможно'"));
    #КонецЕсли	
    
КонецФункции

Функция ПечатьИнформацииДляОплатыДоговора(ОписаниеКоманды) Экспорт
    #Если НЕ ВебКлиент Тогда
        Если ОписаниеКоманды.ОбъектыПечати.Количество() > 1 Тогда
            Предупреждение(НСтр("ru = 'Выполнение данной операции возможно только для одного элемента'"));
            Возврат Неопределено;
        КонецЕсли;
        
        ПараметрКоманды = ОписаниеКоманды.ОбъектыПечати[0];
        
        ПараметрыФормы = Новый Структура;
        ПараметрыФормы.Вставить("ПараметрКоманды", ОписаниеКоманды.ОбъектыПечати);
    	ПараметрыФормы.Вставить("Договор", ПараметрКоманды);

	    ОткрытьФормуМодально("Обработка.мИнформацияДляОплатыДоговора.Форма", ПараметрыФормы,
    		ОписаниеКоманды.ОбъектыПечати);
                
    #Иначе
        Предупреждение(НСтр("ru = 'В веб-клиенте выполнение данной операции невозможно'"));
    #КонецЕсли	
    
КонецФункции

